{
  "kind": "collectionType",
  "collectionName": "tasks",
  "info": {
    "singularName": "task",
    "pluralName": "tasks",
    "displayName": "task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "taskFiles": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date": {
      "type": "string"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "jwt": {
      "type": "text"
    },
    "employee": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::employee.employee",
      "inversedBy": "tasks"
    },
    "status": {
      "type": "string"
    },
    "priority": {
      "type": "string"
    },
    "department": {
      "type": "string"
    },
    "isApproved": {
      "type": "string",
      "default": "pending"
    },
    "employee_checkeds": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::employee.employee",
      "mappedBy": "task"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project",
      "mappedBy": "task"
    },
    "comment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::comment.comment",
      "inversedBy": "task"
    }
  }
}
